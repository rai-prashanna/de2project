server {
    listen  80;
    listen [::]:80;
    server_name one;

    root /var/www/prashanna.com;
    index index.html;

    location / {
        try_files $uri $uri/ =404;
    }
}

 sudo ln -s /etc/nginx/sites-available/prashanna.com /etc/nginx/sites-enabled/